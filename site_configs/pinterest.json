{
    "*://*.pinterest.com/*": {
        "name": "Pinterest",
        "logout": {
            "cookies": ["_pinterest_sess", "_b"]
        },
        "login": {
            "urls": [
                "http://www.pinterest.com/",
                "https://www.pinterest.com/login/"
            ],
            "formURL": "https://www.pinterest.com/login/",
            "method": "POST",
            "usernameField": "username_or_email",
            "passwordField": "password",
            "submitButton": "button:contains('Log In')",
            "check": ".profileName, .myBoards, a:contains('Log Out')"
        }
    }
}