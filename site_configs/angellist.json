{
    "*://*.angel.co/*": {
        "name": "AngelList",
        "logout": {
            "cookies": ["_angellist"]
        },
        "login": {
            "urls": ["https://angel.co/login"],
            "formURL": "https://angel.co/users/login",
            "method": "POST",
            "usernameField": "user[email]",
            "passwordField": "user[password]",
            "hasHiddenInputs": true,
            "check": ".user_image, a:contains('Logout')"
        }
    }
}

